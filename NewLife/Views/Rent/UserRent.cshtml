@model Rent

@{
    ViewData["Title"] = "User Rent";
}

<h2>User Rent</h2>

<form asp-action="UserRent" method="post">
    <input asp-for="User.User_Email" hidden />
    <input asp-for="User.User_Password" hidden />
    <input asp-for="User.User_Phone" hidden />



    <div class="form-group">
        <label asp-for="Rent_Time" class="control-label"></label>
        <input asp-for="Rent_Time" class="form-control" />
        <span asp-validation-for="Rent_Time" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Back_Time" class="control-label"></label>
        <input asp-for="Back_Time" class="form-control" />
        <span asp-validation-for="Back_Time" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Car.Car_Name" class="control-label"></label>
        <input asp-for="Car.Car_Name" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="User.User_Name" class="control-label"></label>
        <input asp-for="User.User_Name" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="User.User_Surname" class="control-label"></label>
        <input asp-for="User.User_Surname" class="form-control" readonly />
    </div>
    <!-- Diğer alanlar -->
    <div class="form-group">
        <input type="submit" value="Kaydet" class="btn btn-primary" />
    </div>
</form>
